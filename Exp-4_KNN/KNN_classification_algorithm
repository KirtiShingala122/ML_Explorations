      "cell_type": "code",
      "source": [
        "import math\n",
        "def distance(x1, x2):\n",
        "    distances = 0.0\n",
        "    for i in range(len(x1)):\n",
        "        distances += (x1[i] - x2[i])**2\n",
        "    return math.sqrt(distances)"
      ],
      "metadata": {
        "id": "BCiA_yjlz1de"
      },
      "execution_count": null,
      "outputs": []
    },
